{
  "name": "@craigmcc/libraries",
  "version": "1.0.0",
  "description": "Library Management application (full stack)",
  "main": "dist/server.js",
  "scripts": {
    "develop:dev": "concurrently \"npm:server:build:watch\" \"npm:start:dev\"",
    "develop:prod": "concurrently \"npm:server:build:watch\" \"npm:start:prod\"",
    "run": "cross-env NODE_ENV=production node dist/server.js",
    "run:dev": "cross-env NODE_ENV=development node dist/server.js",
    "run:prod": "cross-env NODE_ENV=production node dist/server.js",
    "server:build": "tsc",
    "server:build:watch": "tsc --watch --preserveWatchOutput",
    "start:dev": "cross-env NODE_ENV=development nodemon dist/server.js",
    "start:prod": "cross-env NODE_ENV=production nodemon dist/server.js",
    "test": "cross-env NODE_ENV=test mocha -r ts-node/register src/**/*.test.ts",
    "test:watch": "cross-env NODE_ENV=test mocha --watch -r ts-node/register src/**/*.test.ts",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craigmcc/libraries.git"
  },
  "keywords": [
    "express",
    "javascript",
    "node",
    "postgres",
    "react",
    "sequelize",
    "typescript"
  ],
  "author": "craigmcc",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/craigmcc/libraries/issues"
  },
  "homepage": "https://github.com/craigmcc/libraries#readme",
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.15",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.2",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.37",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.1.3",
    "chai": "^4.3.4",
    "concurrently": "^6.0.0",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "sqlite3": "^5.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csvtojson": "^2.0.10",
    "custom-env": "^2.0.1",
    "date-fns": "^2.19.0",
    "dotenv": "^8.2.0",
    "express": "^5.0.0-alpha.8",
    "pg": "^8.5.1",
    "sequelize": "^6.6.2",
    "sequelize-typescript": "^2.1.0"
  }
}
